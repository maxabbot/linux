{
  "__separator__": "Archinstall user configuration â€” adjust to taste before running archinstall --config",
  "additional-repositories": ["multilib"],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "Grub",
  "config_version": "2.8.1",
